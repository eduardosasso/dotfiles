[user]
	name = Eduardo Sasso
	email = eduardo.sasso@gmail.com
	signingkey = /Users/eduardosasso/.ssh/id_ed25519
[core]
	pager = delta
[interactive]
	diffFilter = delta --color-only
[delta]
	navigate = true
	light = false
	line-numbers = true
	side-by-side = false
	decorations = false
	file-style = plain
	hunk-header-style = omit
	minus-line-numbers-left-style = "#4C566A"
	minus-line-numbers-right-style = "#4C566A"
	line-numbers-minus-style = "#ff6b6b"
	line-numbers-plus-style = "#51cf66"
	line-numbers-left-style = "#666666"
	line-numbers-right-style = "#666666"
	file-decoration-style = none
	zero-style = normal
	line-numbers-zero-style = 
	file-modified-label = 
[gpg]
	format = ssh
[commit]
	gpgsign = true
[pull]
	rebase = true
